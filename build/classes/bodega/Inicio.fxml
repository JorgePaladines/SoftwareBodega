<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="border" prefHeight="400.0" prefWidth="674.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bodega.InicioController">
   <center>
      <AnchorPane id="AnchorPane" fx:id="anchor" prefHeight="200" prefWidth="320">
         <children>
            <VBox alignment="TOP_CENTER" layoutX="245.0" layoutY="128.0" prefHeight="120.0" prefWidth="184.0" spacing="10.0">
               <children>
                    <Button fx:id="verInventario" onAction="#handleButtonAction" text="Ver Inventario" />
                  <Button fx:id="insertarProducto" mnemonicParsing="false" onAction="#insertarNuevo" text="Administrar Producto" />
                  <Button fx:id="retirarProducto" mnemonicParsing="false" onAction="#retirarProd" text="Eliminar del Inventario" />
               </children>
            </VBox>
         </children></AnchorPane>
   </center>
</BorderPane>
